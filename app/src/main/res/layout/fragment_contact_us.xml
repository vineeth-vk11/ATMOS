<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:background="@drawable/shapecontactus"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"
        android:id="@+id/organisingBody"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textColor="@color/background"
        app:layout_constraintTop_toTopOf="parent"
        android:text="Organising Body"
        android:fontFamily="@font/rajdhani"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toStartOf="@id/tvTechSenate"
        app:layout_constraintStart_toStartOf="parent">
    </TextView>


    <TextView
        android:id="@+id/tvTechSenate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:textSize="20sp"
        android:text="Tech Senate"
        android:paddingLeft="12dp"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"
        android:fontFamily="@font/rajdhani"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingRight="12dp"
        android:layout_marginTop="12dp"
        android:textColor="@color/secondary"
        app:layout_constraintStart_toEndOf="@id/organisingBody"
        app:layout_constraintEnd_toEndOf="parent"
        >
    </TextView>
    <FrameLayout
        android:id="@+id/nestedFragHolder"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/tvTechSenate"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>